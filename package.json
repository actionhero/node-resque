{
  "author": "Evan Tahler <evantahler@gmail.com>",
  "name": "node-resque",
  "description": "an opinionated implementation of resque in node",
  "license": "Apache-2.0",
  "version": "7.0.1",
  "homepage": "http://github.com/actionhero/node-resque",
  "repository": {
    "type": "git",
    "url": "git://github.com/actionhero/node-resque.git"
  },
  "main": "dist/index",
  "types": "dist/index",
  "keywords": [
    "delayed",
    "queue",
    "resque",
    "redis",
    "work",
    "worker",
    "background",
    "job",
    "task"
  ],
  "engines": {
    "node": ">= 8"
  },
  "dependencies": {
    "ioredis": "^4.16.0"
  },
  "devDependencies": {
    "@types/ioredis": "^4.14.9",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.1",
    "jest": "^25.1.0",
    "node-schedule": "^1.3.2",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.6.2",
    "typedoc": "^0.16.11",
    "typescript": "^3.8.3"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/utils"
    ],
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    }
  },
  "scripts": {
    "prepare": "npm run build && npm run docs",
    "pretest": "npm run lint",
    "lint": "prettier --check src/*/**.ts __test__/*/**.ts examples/*/**.ts",
    "pretty": "prettier --write src/*/**.ts __test__/*/**.ts examples/*/**.ts",
    "test": "jest",
    "build": "tsc --declaration",
    "docs": "typedoc --out docs --theme default"
  }
}
