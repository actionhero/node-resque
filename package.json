{
  "author": "Evan Tahler <evantahler@gmail.com>",
  "name": "node-resque",
  "description": "an opinionated implementation of resque in node",
  "license": "Apache-2.0",
  "version": "6.0.1",
  "homepage": "http://github.com/actionhero/node-resque",
  "repository": {
    "type": "git",
    "url": "git://github.com/actionhero/node-resque.git"
  },
  "main": "dist/index",
  "types": "dist/index",
  "keywords": [
    "delayed",
    "queue",
    "resque",
    "redis",
    "work",
    "worker",
    "background",
    "job",
    "task"
  ],
  "engines": {
    "node": ">= 8"
  },
  "dependencies": {
    "ioredis": "^4.14.1",
    "@types/ioredis": "^4.0.18",
    "@types/jest": "^24.0.22",
    "@types/node": "^12.12.7"
  },
  "devDependencies": {
    "jest": "^24.9.0",
    "node-schedule": "^1.3.2",
    "prettier": "^1.19.1",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.5.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.7.2"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/utils"
    ],
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    }
  },
  "scripts": {
    "prepare": "npm run declaration && npm run docs",
    "pretest": "npm run lint",
    "lint": "prettier --check src/*/**.ts __test__/*/**.ts examples/*/**.ts",
    "pretty": "prettier --write src/*/**.ts __test__/*/**.ts examples/*/**.ts",
    "test": "jest",
    "declaration": "tsc --declaration",
    "docs": "typedoc --out docs --theme default"
  }
}
