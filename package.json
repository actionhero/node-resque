{
  "author": "Evan Tahler <evantahler@gmail.com>",
  "name": "node-resque",
  "description": "an opinionated implementation of resque in node",
  "license": "Apache-2.0",
  "version": "8.2.2",
  "homepage": "http://github.com/actionhero/node-resque",
  "repository": {
    "type": "git",
    "url": "git://github.com/actionhero/node-resque.git"
  },
  "main": "dist/index",
  "types": "dist/index",
  "keywords": [
    "delayed",
    "queue",
    "resque",
    "redis",
    "work",
    "worker",
    "background",
    "job",
    "task"
  ],
  "engines": {
    "node": ">= 8"
  },
  "dependencies": {
    "ioredis": "^4.19.4"
  },
  "devDependencies": {
    "@types/ioredis": "^4.19.2",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "ioredis-mock": "^5.2.0",
    "jest": "^26.6.3",
    "node-schedule": "^2.0.0",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.18",
    "typescript": "^4.1.3"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/utils"
    ],
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    }
  },
  "scripts": {
    "prepare": "npm run build && npm run docs",
    "pretest": "npm run lint && npm run build",
    "lint": "prettier --check src __tests__ examples \"*.md\"",
    "pretty": "prettier --write src __tests__ examples \"**/*.md\"",
    "test": "jest",
    "build": "tsc --declaration",
    "docs": "typedoc --out docs --theme default src/index.ts"
  }
}
